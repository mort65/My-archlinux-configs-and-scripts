#! /bin/bash
## dwm autostart script

exec /usr/bin/setxkbmap us,ir -option 'grp:rctrl_rshift_toggle' &
exec /usr/bin/numlockx &
exec /usr/bin//nitrogen --restore &
sleep 1
exec /usr/bin/gxkb &
exec /usr/bin/compton -b &
killall -q twmnd
exec /usr/bin/twmnd &
exec /usr/bin/volumeicon &
exec /usr/bin/udiskie -2 -s &
exec /usr/bin/clipit &
exec /usr/bin/nm-applet &
sleep 1
exec /usr/bin/setxkbmap us,ir -option 'grp:rctrl_rshift_toggle' &
