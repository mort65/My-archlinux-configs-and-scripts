#!/bin/bash
sleep 1
exec /usr/lib/polkit-gnome/polkit-gnome-authentication-agent-1 &
exec /usr/bin/tilda &
exec /usr/bin/nm-applet &
exec /usr/bin/volumeicon &
exec /usr/bin/udiskie -2 -s &
exec /usr/bin/psensor &
/usr/bin/ps aux | /usr/bin/awk '$12~/^.*wallpaperswitcher.py$/{print $2}' 2> /dev/null | /usr/bin/xargs kill &> /dev/null 
exec /usr/bin/python ~/bin/wallpaperswitcher.py &
exec /usr/bin/compton &
exec /usr/bin/gxkb &
exec /usr/bin/clipit &
exec $HOME/.conky/conky-startup1.sh &
