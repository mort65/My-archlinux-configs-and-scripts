plugin {
    type = menu
    expand = false
    padding = 0
    config {
        image = ~/.config/fbpanel/arch.png
        systemmenu {
        }
        separator {
        }
        menu {
            name = Computer
            icon = computer
            item {
                name = File Manager
                icon = file-manager
                action = pcmanfm
            }
            item {
                name = Terminal
                icon = terminal
                action = lxterminal
            }
            separator {
            }
            item {
                name = logout
                icon = system-log-out
                action = openbox --exit
            }
            item {
                name = Suspend
                icon = system-suspend
                action = /usr/bin/systemctl suspend
            }
            item {
                name = Reboot
                icon = gnome-session-reboot
                action = /usr/bin/systemctl reboot
            }
            item {
                name = Shutdown
                icon = gnome-session-halt
                action = /usr/bin/systemctl poweroff
            }
        }
    }
}
